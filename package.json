{
  "name": "trading-bot",
  "version": "1.0.0",
  "main": "start-bots.sh",
  "scripts": {
    "build": "tsc",
    "start-telegram-bot": "node dist/bots/telegram-trading-bot/index.js",
    "start-tracker-bot": "node dist/bots/tracker-bot/index.js",
    "start-sniper-bot": "node dist/bots/solana-sniper-bot/index.js",
    "test-tracker-bot": "ts-node bots/tracker-bot/index.ts",
    "test-telegram-trading-bot": "ts-node bots/telegram-trading-bot/index.ts",
    "test-solana-sniper-bot": "ts-node bots/solana-sniper-bot/index.ts",
    "test-ai-processor": "ts-node bots/telegram-trading-bot/test-ai-processor.ts",
    "test-twitter": "ts-node bots/twitter-tracker-bot/index.ts",
    "test-transaction": "ts-node bots/telegram-trading-bot/test-transaction.ts",
    "test-gmgn": "ts-node gmgn_api/test.ts",
    "api": "ts-node trades-monitoring/index.ts",
    "api:watch": "nodemon --watch 'trades-monitoring/**/*.ts' --exec 'ts-node' trades-monitoring/index.ts",
    "historical-data": "ts-node trades-monitoring/historical-data-scheduler.ts",
    "watch:telegram-bot": "nodemon --watch 'bots/telegram-trading-bot/**/*.ts' --exec 'ts-node' bots/telegram-trading-bot/index.ts",
    "watch:tracker-bot": "nodemon --watch 'bots/tracker-bot/**/*.ts' --exec 'ts-node' bots/tracker-bot/index.ts",
    "watch:sniper-bot": "nodemon --watch 'bots/solana-sniper-bot/**/*.ts' --exec 'ts-node' bots/solana-sniper-bot/index.ts",
    "dev": "concurrently \"npm run api:watch\" \"npm run watch:telegram-bot\" \"npm run watch:tracker-bot\" \"npm run watch:sniper-bot\" \"npm run watch:historical-data\"",
    "dev:api": "npm run api:watch",
    "api:test": "ts-node trades-monitoring/test-api.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "andy.vaulin@gmail.com",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.4",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.2",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "@duneanalytics/client-sdk": "^0.2.4",
    "@langchain/openai": "^0.4.4",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@notionhq/client": "^2.2.15",
    "@project-serum/anchor": "^0.26.0",
    "@sendgrid/mail": "^8.1.3",
    "@solana/web3.js": "^1.95.5",
    "@types/axios": "^0.14.4",
    "@types/node-cron": "^3.0.11",
    "@types/puppeteer": "^7.0.4",
    "add": "^2.0.6",
    "axios": "^1.7.9",
    "bs58": "^6.0.0",
    "cloudflare-scrape": "^0.0.2",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "https-proxy-agent": "^7.0.5",
    "langchain": "^0.3.19",
    "luxon": "^3.5.0",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.9",
    "playwright": "^1.48.1",
    "puppeteer": "^23.11.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zod": "^3.24.2"
  }
}
